    <fragment
        android:id="@+id/usage_timer_fragment"
        android:name="com.black.app.ui.maintab.main.usagetimer.UsageTimerFragment"
        android:label="UsageTimer"
        tools:layout="@layout/fragment_usage_timer">
        <action
            android:id="@+id/action_select_app"
            app:destination="@id/select_app_dialog"
            app:launchSingleTop="true"
            />
    </fragment>

    <fragment
        android:id="@+id/etc_fragment"
        android:name="com.black.app.ui.maintab.main.etc.EtcFragment"
        android:label="ETC"
        tools:layout="@layout/fragment_etc" />

    <fragment
        android:id="@+id/notification_fragment"
        android:name="com.black.app.ui.maintab.main.notification.NotificationFragment"
        android:label="Notification" />

    <fragment
        android:id="@+id/poke_rogue_fragment"
        android:name="com.black.feature.pokerogue.ui.PokeRogueFragment"
        android:label="@string/poke_rogue" />

    <fragment
        android:id="@+id/floating_fragment"
        android:name="com.black.feature.floatingbutton.ui.FloatingSettingFragment"
        android:label="FloatingButton" />

    <dialog
        android:id="@+id/select_app_dialog"
        android:name="com.black.app.ui.common.selectapp.SelectAppDialogFragment"
        android:label="@string/select_app" />

</navigation>