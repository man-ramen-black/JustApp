            android:id="@+id/action_floating"
            app:destination="@id/floating_fragment"
            app:restoreState="true"
            app:popUpToSaveState="true"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/black_fragment"
        android:name="com.black.app.ui.maintab.main.etc.BlackFragment"
        android:label="@string/fragment_name_black"
        tools:layout="@layout/fragment_black" />

    <fragment
        android:id="@+id/text_editor_fragment"
        android:name="com.black.app.ui.maintab.main.texteditor.TextEditorFragment"
        android:label="TextEditor"
        tools:layout="@layout/fragment_text_editor" />

    <fragment
        android:id="@+id/recycler_view_fragment"
        android:name="com.black.app.ui.maintab.main.recyclerview.RecyclerViewFragment"
        android:label="RecyclerView"
        tools:layout="@layout/fragment_recyclerview" />

    <fragment
        android:id="@+id/retrofit_fragment"
        android:name="com.black.app.ui.maintab.main.retrofit.RetrofitFragment"
